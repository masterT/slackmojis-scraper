{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "required": [
      "name",
      "filename",
      "url"
    ],
    "properties": {
      "name": {
        "type": "string"
      },
      "filename": {
        "type": "string"
      },
      "url": {
        "type": "string",
        "format": "uri"
      }
    }
  }
}
